{
   "name": "graphql-ts-server-auth",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/node": "^8.0.29",
      "@types/uuid": "^8.3.0",
      "gql2ts": "^1.10.1",
      "jest": "^26.6.3",
      "nodemon": "^2.0.7",
      "ts-node": "3.3.0",
      "tslint": "^6.1.3",
      "tslint-config-prettier": "^1.18.0",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "@types/jest": "^26.0.20",
      "bcryptjs": "^2.4.3",
      "graphql-import": "^1.0.2",
      "graphql-request": "^3.4.0",
      "graphql-yoga": "^1.18.3",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.10",
      "ts-jest": "^26.4.4",
      "typeorm": "0.2.29",
      "uuid": "^8.3.2",
      "uuid4": "^2.0.2"
   },
   "scripts": {
      "start": "nodemon --exec ts-node src/index.ts",
      "test": "jest",
